		{
			"name": "Flutter Web (with disabled web security)",
			// "program": "lib/main.dart",
			"request": "launch",
			"type": "dart",
			"args": [
			  "--web-browser-flag",
			  "--disable-web-security"
			]
		  },
		{
			"name": "city_fit (profile mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile"
		},
		{
			"name": "city_fit (release mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release"
		}
	]
}

